{
  "json.format.enable": true,
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "Markdown",
    "typescript",
    "javascript",
    "javasscriptreact",
    "typescriptreact",
    "json",
    "elm"
  ],
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": [
        "elm"
      ],
      "rootPatterns": [
        "elm.json"
      ]
    },
    "sourcekit-lsp": {
      "filetypes": [
        "swift"
      ],
      "command": "sourcekit-lsp"
    },
    "csharp-ls": {
      "command": "csharp-ls",
      "filetypes": [
        "cs"
      ],
      "rootPatterns": [
        "*.csproj",
        ".vim/",
        ".git/",
        ".hg/"
      ]
    }
  }
}
